<div class="container my-5">
  <h1>Registro del Vehiculo</h1>
  <%= simple_form_for @vehicle do |f| %>
    <%= f.input :plate_number, placeholder: 'Numero de placa' %>
    <%= f.input :vehicle_type, placeholder: 'Tipo de vehicle' %>
    <%= f.input :vehicle_branding, placeholder: 'Marca de vehiculo' %>
    <%= f.input :model, placeholder: 'Modelo' %>
    <%= f.submit 'Crear vehiculo', class: "btn btn-success" %>
    <% end %>
</div>

